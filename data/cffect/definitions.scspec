
CffectID := $registryPath

CffectDefinition := {
    start? $command
    tick? $command
    end? $command
    duplicate_handler? $command
}

CffectInstance := {
    id: CffectID
    data: any
    ending_task: delay\TaskID
    tick? {
        enabled: bool
        command: $command
    }
}

EntityEntry := {
    uuid: uuid
    cffects[]: CffectInstance
}